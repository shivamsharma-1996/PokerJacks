<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gtgt.pokerjacks">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.wallet.WithdrawActivity"></activity>
        <activity android:name=".ui.profile.AccConfirmationActivity" />
        <activity android:name=".ui.profile.AddAccountActivity" />
        <activity android:name=".ui.profile.ManageBankAccountActivity" />
        <activity android:name=".ui.profile.ResponsibleGamingActivity" />
        <activity android:name=".ui.profile.vrify_email.VerifyEmailOTPActivity" />
        <activity android:name=".ui.profile.verify_pan.VerifyPanActivity" />
        <activity android:name=".ui.profile.vrify_email.VerifyEmailActivity" />
        <activity android:name=".ui.profile.verify_address.VerifyAddressActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>